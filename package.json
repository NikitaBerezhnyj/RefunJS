{
  "name": "refunjs",
  "version": "1.0.3",
  "description": "React-based framework",
  "type": "module",
  "main": "core/index.js",
  "bin": {
    "refunjs": "src/index.js"
  },
  "files": [
    "core",
    "features",
    "src",
    "LICENSE",
    "SECURITY.md",
    "CODE_OF_CONDUCT.md"
  ],
  "scripts": {
    "start": "node src/index.js",
    "build": "babel src --out-dir dist --copy-files",
    "prepublishOnly": "node scripts/prepare-readme.js",
    "postpublish": "node scripts/restore-readme.js"
  },
  "dependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@inquirer/prompts": "^7.9.0",
    "chalk": "^5.6.2",
    "diff": "^8.0.2",
    "inquirer": "^12.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/preset-env": "^7.28.5",
    "shx": "^0.4.0"
  }
}
